{
	"name": "discord-messages-react",
	"version": "1.0.0",
	"description": "React components to mimic Discord",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"start": "react-scripts start",
		"build-examples": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"build": "rimraf dist && cross-env NODE_ENV=production babel src/lib --extensions \".ts,.tsx\" --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
		"prepublishOnly": "npm run build"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.5",
		"@babel/core": "^7.14.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.14.7",
		"@babel/plugin-transform-typescript": "^7.14.6",
		"@babel/preset-env": "^7.14.7",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.14.5",
		"@types/node": "^15.12.5",
		"@types/react": "^17.0.11",
		"@types/react-dom": "^17.0.8",
		"@types/styled-components": "^5.1.10",
		"@types/twemoji": "^12.1.1",
		"babel-plugin-styled-components": "^1.12.0",
		"cross-env": "^7.0.3",
		"react-scripts": "^4.0.3",
		"rimraf": "^2.7.1",
		"typescript": "^4.3.4",
		"webpack": "^4.44.2"
	},
	"dependencies": {
		"styled-components": "^5.3.0",
		"twemoji": "^13.1.0"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/daimond113/discord-messages-react.git"
	},
	"keywords": [
		"discord",
		"messages",
		"react"
	],
	"author": "https://github.com/daimond113",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/daimond113/discord-messages-react/issues"
	},
	"peerDependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}
